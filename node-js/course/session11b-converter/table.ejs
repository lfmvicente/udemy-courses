<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Table</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="">
    </head>
    <body>
        <table>
            <thead>
                <% header.forEach(column => { %>
                    <th><%= column %></th>
                <% }) %>
            </thead>
            <tbody>
                <% rows.forEach(row => { %>
                    <tr>
                        <% row.forEach(element => { %>
                            <td>
                                <%= element %>
                            </td>    
                        <% }) %>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </body>
</html>